{
  "CompanyLogging": {
    "ServiceName": "orders-api",
    "ServiceVersion": "1.0.0",
    "Environment": "dev",
    "Region": "us-east-1",
    "CorrelationHeader": "X-Correlation-Id",
    "EnableRequestLogging": true,
    "EnableBodyLogging": false,
    "BodySizeLimitBytes": 4096,
    "RequestLoggingExcludedPaths": [ "/health", "/metrics", "/favicon.ico" ],
    "RequestLoggingWarnAboveStatus": 400,
    "RequestLoggingErrorAboveStatus": 500,
    "Redaction": {
      "SensitiveKeys": [
        "password", "passwd", "secret", "token", "apikey", "api_key",
        "authorization", "auth", "credential", "connectionstring",
        "privatekey", "clientsecret"
      ],
      "RedactedValue": "***"
    },
    "Sampling": {
      "SampleSuccessRequests": false,
      "SuccessSampleRate": 10
    },
    "Elasticsearch": {
      "Enabled": true,
      "Uri": "http://localhost:9200",
      "AutoRegisterTemplate": true,
      "BatchPostingLimit": 1000,
      "PeriodSeconds": 2
    },
    "Async": {
      "Enabled": true,
      "BufferSize": 10000,
      "BlockWhenFull": false
    },
    "MetaKeyWhitelist": [],
    "Telemetry": {
      "Enabled": true,
      "ServiceName": "orders-api",
      "ServiceVersion": "1.0.0",
      "Otlp": {
        "Enabled": false,
        "Endpoint": "http://localhost:4317",
        "Protocol": "grpc"
      },
      "ElasticApm": {
        "Enabled": false,
        "ServerUrl": "http://localhost:8200",
        "ApiKey": "",
        "SecretToken": ""
      },
      "Tracing": {
        "Enabled": true,
        "SampleRatio": 1.0,
        "InstrumentAspNetCore": true,
        "InstrumentHttpClient": true,
        "InstrumentEntityFramework": false,
        "InstrumentSqlClient": false,
        "AdditionalSources": []
      },
      "Metrics": {
        "Enabled": true,
        "InstrumentAspNetCore": true,
        "InstrumentHttpClient": true,
        "InstrumentRuntime": true,
        "AdditionalMeters": []
      }
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "AllowedHosts": "*"
}
